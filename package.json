{
  "name": "apollo-aperture-fresh",
  "author": "Steven Jung, Jonathan Peralez, Raffi Nakashian, Mike Huynh",
  "version": "0.1.0",
  "private": true,
  "main": "public/electron.js",
  "homepage": "./",
  "dependencies": {
    "@babel/parser": "^7.4.5",
    "@babel/traverse": "^7.4.5",
    "@babel/types": "^7.4.4",
    "babel-plugin-wildcard": "^5.0.0",
    "glob": "^7.1.4",
    "webpack": "^4.35.2"
  },
  "devDependencies": {
    "babel-jest": "^24.8.0",
    "electron": "^5.0.2",
    "electron-builder": "^20.43.0",
    "eslint": "^6.0.1",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-config-prettier": "^6.0.0",
    "eslint-import-resolver-webpack": "^0.11.1",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.2",
    "jest": "^24.8.0",
    "prettier": "^1.18.2",
    "typescript": "^3.5.1",
    "webpack-cli": "^3.3.5",
    "webpack-dev-server": "^3.7.2"
  },
  "scripts": {
    "webpack-dev": "webpack-dev-server --open",
    "electron-dev": "concurrently \"npm run start\" \"wait-on http://localhost:3000 && electron .\"",
    "electron-pack": "build --c.extraMetadata.main=build/electron.js",
    "preelectron-pack": "npm run build"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "build": {
    "appId": "com.example.electron-cra",
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    }
  }
}
